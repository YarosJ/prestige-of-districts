{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": ["eslint-plugin-babel"],
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "overrides": [{
        "files": ["*.ts"],
        "parser": "@typescript-eslint/parser",
        "plugins": ["@typescript-eslint"]
    }]
}
